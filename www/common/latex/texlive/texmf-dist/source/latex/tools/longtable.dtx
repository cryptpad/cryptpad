% \begin{macro}{\LT@no@pgbk}
% \changes{v4.05}{1996/11/12}
%      {Macro added}
% Can simplify the standard "\@no@pgbk" as this is vmode only
% but then need to close the "\noalign".
%    \begin{macrocode}
\def\LT@no@pgbk#1[#2]{\penalty #1\@getpen{#2}\ifnum`{=0\fi}}

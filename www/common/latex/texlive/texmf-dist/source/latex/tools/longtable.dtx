%     Caption `Table ?: \meta{caption}',
%     but no entry in the list of tables.\\
% "\caption*"\marg{caption}&
%     Caption `\meta{caption}', but no entry in the list of tables.\\[5pt]
% \multicolumn{2}{c}{%^^A
%        \textbf{Commands available at the start of a row}}\\*
% \hline
% "\pagebreak"&
%     Force a page break.\\*
